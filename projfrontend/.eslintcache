 createOrder(userId, token, orderData);\n\n          cartEmpty(() => {\n            console.log(\"DID WE CRASH\");\n          });\n          setReload(!reload);\n        })\n        .catch((err) => {\n          setInfo({ loading: false, success: false });\n          console.log(\"PAYMENT FAILED\");\n        });\n    });\n  };\n\n  const getAmount = () => {\n    let amount = 0;\n    products.map((product) => {\n      amount = amount + product.price;\n    });\n    return amount;\n  };\n\n  const showbtdropIn = () => {\n    return (\n      <div>\n        {info.clientToken !== null && products.length > 0 ? (\n          <div>\n            <DropIn\n              options={{ authorization: info.clientToken }}\n              onInstance={(instance) => (info.instance = instance)}\n            />\n            <button className=\"btn btn-success\" onclick={onPurchased}>\n              buy\n            </button>\n          </div>\n        ) : (\n          <h2> Please Login or Add Something to Cart</h2>\n        )}\n      </div>\n    );\n  };\n  useEffect(() => {\n    getmeToken(userId, token);\n  }, []);\n  return (\n    <div>\n      <h3>your Bill is {getAmount()} $</h3>\n      {showbtdropIn()}\n    </div>\n  );\n};\n\nexport default Payment;\n","/home/akash/GitHub/lcoproject/projfrontend/src/core/helper/paymentHelper.js",[],"/home/akash/GitHub/lcoproject/projfrontend/src/core/helper/orderHelper.js",[],["152","153"],{"ruleId":"154","replacedBy":"155"},{"ruleId":"156","replacedBy":"157"},{"ruleId":"158","severity":1,"message":"159","line":3,"column":10,"nodeType":"160","messageId":"161","endLine":3,"endColumn":13},{"ruleId":"158","severity":1,"message":"162","line":9,"column":10,"nodeType":"160","messageId":"161","endLine":9,"endColumn":15},{"ruleId":"158","severity":1,"message":"163","line":27,"column":5,"nodeType":"160","messageId":"161","endLine":27,"endColumn":10},{"ruleId":"158","severity":1,"message":"164","line":29,"column":5,"nodeType":"160","messageId":"161","endLine":29,